    <ul>
    <% if signed_in? %>
      <li><%= button_to "Sign out", session_url, method: :delete%></li>
      <li><%= button_to "Make New Blog", new_blog_url, method: :post%></li>
    <% else %>
      <li><%= link_to "Log in", new_session_url %></li>
      <li><%= link_to "Sign up", new_user_url %></li>
    <% end %>
    </ul>

<% if flash[:errors] %>
  <%= flash[:errors].join("<br>").html_safe %>
<% end %>
